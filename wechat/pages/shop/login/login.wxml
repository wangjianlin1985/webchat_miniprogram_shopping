
<import src="../common/head.wxml" />
<import src="../common/foot.wxml" />
<view class="container">
 
  <view class="page-body">
    <view class="classname">
    <image src="../../../images/ff.jpeg" class="imggg"></image>
    </view>
    <view class="page-section">
      <view class="weui-cells weui-cells_after-title">
        <view class="weui-cell weui-cell_input">
          <view class="weui-cell__hd">
            <view class="weui-label">用户名</view>
          </view>
          <view class="weui-cell__bd">
            <input class="weui-input" type="text" placeholder="请输入用户名" name="name"  bindinput="nameInput" value='{{name}}'></input>
          </view>
        </view>
        <view class="weui-cell weui-cell_input">
          <view class="weui-cell__hd">
            <view class="weui-label">密码</view>
          </view>
          <view class="weui-cell__bd">
            <input class="weui-input" type="text" placeholder="请输入密码"  name="pass" password value='{{pass}}'  bindinput="passwordInput"></input>
          </view>
        </view>
      </view>
      <view class="btn-area delu">
        <button type="primary" bindtap="denglu">登录</button>
      </view>
    </view>
  </view>
 
 <ul class="ull" bindtap="toRes">
  <li>
    没有账号去注册
  </li>
 </ul>

  
</view>
 
